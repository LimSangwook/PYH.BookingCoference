	                    } else strHtml += "<td class='" + (j == 1 ? "sun" : (j == 7 ? "sat" : "")) + "'><span>" + ndays + "</span></td>";
	                      ndays++
	                      
	                    };
	                } else strHtml += "<td>&nbsp;</td>";
	              };
	              strHtml += "</tr>"
	            };
	            strHtml += "</table>";
	            document.getElementById("calenda").innerHTML = strHtml;
	            document.getElementById("calenda2").innerHTML = strHtml;
       			
       		}
          };
          function func_Time() {
            var now = new Date();
            var curhour = (now.getHours() >= 12 ? now.getHours() - 12 : now.getHours());
            return (now.getHours() >= 12 ? "PM" : "PM") + (curhour == 0 ? 12 : curhour) + " : " + (now.getMinutes() < 10 ? "0" + now.getMinutes() : now.getMinutes());
            now = null
          };

